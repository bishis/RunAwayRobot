<?xml version="1.0"?>
<robot name="my_robot">
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.17"/>
      </geometry>
    </visual>
  </link>

  <link name="laser">
    <visual>
      <geometry>
